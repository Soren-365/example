{
  "private": true,
  "scripts": {
    "dev": "cross-env DB_LOCATION=local_db next dev --turbo",
    "dev-no-turbo": "cross-env DB_LOCATION=prod_db next dev",
    "dev-remote": "cross-env DB_LOCATION=prod_db next dev",
    "build": "cross-env DB_LOCATION=prod_db next build",
    "start": "cross-env DB_LOCATION=prod_db next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "db-local-types": "supabase gen types typescript --local > lib/types/database.types.ts ",
    "tailwind-link": "npx tailwindcss -i ./lib/styles/input.css -o ./app/globals.css --watch"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.3",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/supabase-js": "^2.8.0",
    "framer-motion": "^10.12.9",
    "isomorphic-dompurify": "^1.8.0",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "^13.4.19",
    "postcss": "^8.4.21",
    "supabase": "^1.42.8",
    "tailwindcss": "^3.2.7",
    "typescript": "4.7.4"
  }
}
