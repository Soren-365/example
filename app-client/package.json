{
  "private": true,
  "scripts": {
    "dev-turbo": "next dev --turbo",
    "dev-local": "cross-env DB_LOCATION=local_db next dev",
    "dev": "cross-env DB_LOCATION=prod_db next dev",
    "build": "cross-env DB_LOCATION=prod_db next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "db-local-types": "supabase gen types typescript --local > lib/types/db-generated.ts ",
    "tailwind-link": "npx tailwindcss -i ./lib/styles/input.css -o ./app/globals.css --watch"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/supabase-js": "^2.8.0",
    "next": "^13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.21",
    "supabase": "^1.42.8",
    "tailwindcss": "^3.2.7",
    "typescript": "4.7.4"
  }
}
